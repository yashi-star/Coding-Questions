
class Solution {
public:
    bool isValid(string s) {
        int n=s.size();
     stack<int>st;
     for(int i=0;i<n;i++){
         if(s[i]=='('||s[i]=='{'||s[i]=='['){
             st.push(s[i]);
         }
         else if(!st.empty()){
             if(st.top()=='('&&s[i]!=')')
             return false;
             if(st.top()=='{'&&s[i]!='}')
             return false;
             if(st.top()=='['&&s[i]!=']')
             return false;
             st.pop();
         }
          else
             return false;
     }
    if(st.empty())
       return true;
       else
       return false;
        }
};
